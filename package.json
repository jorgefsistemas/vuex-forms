{
  "name": "vuex-forms",
  "version": "0.0.7",
  "description": "Comprehensive form management for Vue.js, built with Vuex in mind.",
  "author": "Andy Crockett",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "andycrockett/vuex-forms",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "docs": "rm -rf docs-dist && node build/build.js",
    "bundle:web": "rm -rf dist && NODE_ENV=production BUILD=web webpack --config build/webpack.bundle.conf.js",
    "bundle:lib": "rm -rf lib && BUILD=lib babel src --out-dir lib && cpx 'src/templates/**/*' 'lib/templates'",
    "build": "npm run bundle:lib && npm run bundle:web"
  },
  "dependencies": {
    "merge": "^1.2.0",
    "vuelidate": "^0.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "axios": "^0.21.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^0.4.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "node-sass": "^4.5.3",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.5",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "template-html-loader": "^1.0.0",
    "url-loader": "^0.5.8",
    "vue": "^2.3.3",
    "vue-analytics": "^4.1.2",
    "vue-highlightjs": "^1.3.3",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vuex": "^2.3.1",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
